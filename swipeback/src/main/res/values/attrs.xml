<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeBackLayout">
        <attr name="sbl_enable" format="boolean|reference" />
        <attr name="sbl_activityIsAlreadyTranslucent" format="boolean|reference" />
        <attr name="sbl_takeOverActivityAnim" format="boolean|reference" />
        <attr name="sbl_takeOverActivityAnimDuration" format="integer|reference" />
        <attr name="sbl_forceEdge" format="boolean|reference" />
        <attr name="sbl_onlyEdge" format="boolean|reference" />
        <attr name="sbl_shadowEnable" format="boolean|reference" />
        <attr name="sbl_shadowStartColor" format="color|reference" />
        <attr name="sbl_shadowSize" format="dimension|reference" />
        <attr name="sbl_maskAlpha" format="integer|reference" />
        <attr name="sbl_autoFinishedVelocityLimit" format="float|reference" />
        <attr name="sbl_factor" format="float|reference" />
        <attr name="sbl_direction">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="4" />
            <enum name="bottom" value="8" />
        </attr>
    </declare-styleable>
</resources>